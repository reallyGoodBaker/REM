        "skipLibCheck": true,
        "noEmitOnError": true,
        "strict": true,
        "esModuleInterop": false,
        "outDir": "../protocol_dist",
        "rootDir": "./",
        "allowJs": true,
        "noImplicitAny": true,
        "declaration": true,
    },
    "include": ["./**/*"]
}